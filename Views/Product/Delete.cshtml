@model Real_Estate.Core.Application.Dto.ProductDto
<div>Are you sure you want to delete this Product?</div>
<div>
    <h4>@Model.ProductCode</h4>
    <h4>@Model.Name</h4>
    <h4>@Model.Price</h4>
</div>
<form asp-action="Delete">
    <input type="hidden" value="@Model.Id"/>
    <input class="btn btn-danger" type="submit" value="Delete"/>
    <li>
        <a asp-controller="Product" asp-action="List"><button>No</button></a>
        </li>
</form>